{
  "branches": [
    {
      "case": {
        "$eq": [
          "scores",
          100
        ]
      },
      "then": "100 !"
    },
    {
      "case": {
        "$and": [
          {
            "$gte": [
              "scores",
              80
            ]
          },
          {
            "$lt": [
              "scores",
              90
            ]
          }
        ]
      },
      "then": "Doing pretty well."
    }
  ],
  "default": "No scores found."
}
